{
  "name": "MoeImgTag",
  "author": [
    "[https://github.com/dragon-fish dragon-fish]"
  ],
  "url": "https://github.com/moegirlwiki/mediawiki-extension-MoeImgTag",
  "descriptionmsg": "imgtag-desc",
  "version": "1.0.0",
  "license-name": "MIT",
  "type": "parserhook",
  "AutoloadNamespaces": {
    "Moegirl\\ImgTag\\": "includes/"
