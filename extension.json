  "Hooks": {
    "ParserFirstCallInit": "main",
    "ParserBeforeInternalParse": "main"
  },
  "MessagesDirs": {
    "MoeImgTag": [
      "i18n"
    ]
  },
  "ExtensionMessagesFiles": {
    "MoeImgTagAlias": "MoeImgTag.i18n.php"
  },
  "config": {
    "MoeImgTagWhitelist": {
      "descriptionmsg": "config-desc-addimgtagwhitelist",
