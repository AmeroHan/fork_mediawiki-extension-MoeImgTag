    "MoeImgTagBlacklist": {
      "descriptionmsg": "config-desc-addimgtagblacklist",
      "value": []
    },
    "MoeImgTagFixSelfClosing": {
      "descriptionmsg": "config-desc-enable-self-closing",
      "value": false
    }
  },
  "manifest_version": 2
